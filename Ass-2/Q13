#include <stdio.h>
int main()
{
    int i, total;

    printf("enter size for array: \n = ");
    scanf("%d", &total);

    int arr[total];

    for(i=0 ; i<total ; i++)
    {
        printf("\nvalue no %d = ", i+1);
        scanf("%d", &arr[i]);
    }
        
    printf("\ninput: arr[] = {");
    for(i=0 ; i<total ; i++)
    {
    printf(" %d ", arr[i]);
    }
    printf("}");

    printf("\n\noutput: arr[] = {");
    for(i=0 ; i<total ; i++)
    {
        printf(" %d ", arr[i-1]);
    }
    printf("}");

    

    
    
    
    
    
return 0;
}
